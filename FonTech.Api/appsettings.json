{
  "RabbitMqSettings":{
    "QueueName": "queue",
    "RoutingKey": "fontech.topic",
    "ExchangeName": "fontech.exchange"
  },
  "ConnectionStrings:PostgresSQL": "Server=localhost;Port=5432;Database=FonTech;User Id=admin;Password=admin",
  "Serilog": {
    "Using": ["Serilog.Sinks.File", "Serilog.Sinks.Console"],
    "MinimumLevel": {
      "Default": "Warning",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [{
      "Name": "Console"
    },
      {
        "Name": "File",
        "Args": {
          "path": "/logs/log-.txt",
          "rollOnFileSizeLimit": true,
          "formatter": "Serilog.Formatting.Compact.CompactJsonFormatter,Serilog.Formatting.Compact",
          "rollingInterval": "Day"
        }
      }],
    "Enrich": ["FromLogContext", "WithThreadId", "WithMachineName"]
  },
  "Jwt": {
    "Issuer": "FonTech",
    "Audience": "FonTech",
    "Authority": "FonTech",
    "JwtKey": "VDdYF0TsFr2zAIMuNAzEgIDxaEXuO7bm",
    "Lifetime": 15,
    "RefreshTokenValidityInDays": 7
  },
  "AllowedHosts": "*"
}
